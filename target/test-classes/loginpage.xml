<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sauce Demo Application">
	<test thread-count="5"
		name="Sauce Demo Regression Suite Test Cases">
		<parameter name="browsername" value="incognito"></parameter>
		<groups>
			<run>
				<include name="RegressionTesting"></include>
			</run>
		</groups>
		<classes>
			<class name="com.saucedemo.TestLayer.LoginPageTest" />
			<class name="com.saucedemo.TestLayer.ProductPageTest" />
		</classes>
	</test> <!-- Sauce Demo Regression Suite Test Cases -->
</suite> <!-- Sauce Demo Application -->
